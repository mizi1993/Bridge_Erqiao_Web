<!DOCTYPE hibernate-mapping
        SYSTEM
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.whut.erqiao.entity">
	<class name="InfoXielasuo" table="infoXielasuo">
		<id name="id">
			<generator class="native" />
		</id>
		<property name="date" />
		<property name="time" />
		<property name="position" />
		<property name="jianzhenqi_level"/>
		<property name="jianzhenqiDes"/>
		<property name="jianzhenqiJson"/>
		<property name="cuoweiDes"/>
		<property name="cuoweiJson"/>
		<property name="hutao_level"/>
		<property name="hutaoDes"/>
		<property name="hutaoJson"/>
		<property name="suojiaDes"/>
		<property name="suojiaJson"/>
		<property name="rest_level"/>
		<property name="restDes"/>
		<property name="restJson"/>
		<property name="other"/>
		<many-to-one name="task" class="Task" column="taskId"></many-to-one>
	</class>

</hibernate-mapping>
