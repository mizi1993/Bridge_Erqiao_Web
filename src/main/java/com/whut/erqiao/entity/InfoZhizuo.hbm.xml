<!DOCTYPE hibernate-mapping
        SYSTEM
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.whut.erqiao.entity">
	<class name="InfoZhizuo">
		<id name="id">
			<generator class="native" />
		</id>
		<property name="date" />
		<property name="time" />
		<property name="position" />
		<property name="no"/>
		<property name="luoshuan_level"/>
		<property name="luoshuanJson"/>
		<property name="luoshuanDes"/>
		<property name="weiyi_level"/>
		<property name="weiyiJson"/>
		<property name="weiyiDes"/>
		<property name="xiushi_level"/>
		<property name="xiushiJson"/>
		<property name="xiushiDes"/>
		<property name="songdong_level"/>
		<property name="songdongJson"/>
		<property name="songdongDes"/>
		<property name="fangzhenkuaiJson"/>
		<property name="fangzhenkuaiDes"/>
		<property name="other"/>
		<many-to-one name="task" class="Task" column="taskId"></many-to-one>
	</class>

</hibernate-mapping>
