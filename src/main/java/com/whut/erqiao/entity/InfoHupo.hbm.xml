<!DOCTYPE hibernate-mapping
        SYSTEM
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.whut.erqiao.entity">
	<class name="InfoHupo" table="infoHupo">
		<id name="id">
			<generator class="native"/>
		</id>
		<property name="date" />
		<property name="time" />
		<property name="position" />
		<property name="chenjiangDes" />
		<property name="chenjiangJson" />
		<property name="kailie_level" />
		<property name="kailieDes" />
		<property name="kailieJson" />
		<property name="waiyi_level" />
		<property name="waiyiDes" />
		<property name="waiyiJson" />	
		<property name="tanta_level" />	
		<property name="tantaDes" />
		<property name="tantaJson" />
		<many-to-one name="task" class="Task" column="taskId"></many-to-one>
	</class>

</hibernate-mapping>
