<!DOCTYPE hibernate-mapping
        SYSTEM
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.whut.erqiao.entity">
	<class name="InfoMaogukuai" table="infoMaogukuai">
		<id name="id">
			<generator class="native"/>
		</id>
		<property name="date" />
		<property name="time" />
		<property name="struct" />
		<property name="type" />
		<property name="dun_no" />
		<property name="up_down" />
		<property name="no" />
		<property name="diaoqiDes" />
		<property name="diaoqiJson" />
		<property name="bianxingDes" />
		<property name="bianxingJson" />
		<property name="hunningtuDes" />
		<property name="hunningtuJson" />
		<property name="maoshuanDes" />
		<property name="maoshuanJson" />
		<property name="jiaocengDes" />
		<property name="jiaocengJson" />
		<property name="other" />
		<many-to-one name="task" class="Task" column="taskId"></many-to-one>
	</class>

</hibernate-mapping>
