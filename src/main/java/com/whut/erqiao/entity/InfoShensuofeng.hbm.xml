<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.whut.erqiao.entity">
	<class name="InfoShensuofeng" table="infoShensuofeng">
		<id name="id">
			<generator class="native" />
		</id>
		<property name="date" />
		<property name="time" />
		<property name="position" />
		<property name="type" />
		<property name="chijianju" />
		<property name="shensuoliang" />
		<property name="goujian_level" />
		<property name="goujianJson" />
		<property name="goujianDes" />
		<property name="xiangjiaodai_level" />
		<property name="xiangjiaodaiJson" />
		<property name="xiangjiaodaiDes" />
		<property name="shuchi_level" />
		<property name="shuchiJson" />
		<property name="shuchiDes" />
		<property name="duse_level" />
		<property name="duseJson" />
		<property name="duseDes" />
		<property name="fengpingzheng_level" />
		<property name="fengpingzhengJson" />
		<property name="fengpingzhengDes" />
		<many-to-one name="task" class="Task" column="taskId"></many-to-one>
	</class>
</hibernate-mapping>
