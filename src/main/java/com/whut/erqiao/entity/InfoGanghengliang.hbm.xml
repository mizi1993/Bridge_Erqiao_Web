<!DOCTYPE hibernate-mapping
        SYSTEM
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.whut.erqiao.entity">
	<class name="InfoGanghengliang" table="infoGanghengliang">
		<id name="id">
			<generator class="native"/>
		</id>
		<property name="date" />
		<property name="time" />
		<property name="no" />
		<property name="position" />
		<property name="item" />
		<property name="data" />
		<property name="defect_position" />
		<property name="other" />
		<property name="level" />
		<property name="ganghengliangJson" />
		<many-to-one name="task" class="Task" column="taskId"></many-to-one>
	</class>

</hibernate-mapping>
