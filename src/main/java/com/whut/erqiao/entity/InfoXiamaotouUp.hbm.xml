		<property name="hutao_level"/>
		<property name="hutaoDes"/>
		<property name="hutaoJson"/>
		<property name="danggai_level"/>
		<property name="danggaiDes"/>
		<property name="danggaiJson"/>
		<property name="xiekuai_level"/>
		<property name="xiekuaiDes"/>
		<property name="xiekuaiJson"/>
		<property name="tietao_level"/>
		<property name="tietaoDes"/>
		<property name="tietaoJson"/>
		<property name="zuliquan_level"/>
		<property name="zuliquanDes"/>
		<property name="zuliquanJson"/>
		<property name="chuli"/>
		<property name="other"/>
		<many-to-one name="task" class="Task" column="taskId"></many-to-one>
	</class>

</hibernate-mapping>
